parameters:
    inferPrivatePropertyTypeFromConstructor: true

    ignoreErrors:

        -
            message: '/Else branch is unreachable because previous condition is always true./'
            path: 'src/Graph/GraphSearch.php'

        -
            message: '/Cannot call method .* on [^ ]*|null./'
            path: 'src/Graph/GraphSearch.php'

        -
            message: '/Access to private property \$edges of parent class Smalldb\\StateMachine\\Graph\\NestedGraph./'
            path: 'src/Graph/NestedGraph.php'

        -
            message: '/Parameter #2 $sourceStateName of method Smalldb\\StateMachine\\Definition\\Builder\\StateMachineDefinitionBuilder::addTransition() expects string, string|null given./'
            path: 'src/AnnotationReader.php'

        -
            message: '/Parameter #1 \$constant of method Doctrine\\Common\\Annotations\\AnnotationReader::getConstantAnnotations() expects ReflectionClassConstant, ReflectionClassConstant|false given./'
            path: 'src/Utils/DeepAnnotationReader.php'
